{
  "name": "@hashicorp/ember-flight-icons",
  "version": "1.0.0",
  "description": "The Ember addon for the HashiCorp Flight SVG icon set",
  "keywords": [
    "ember-addon",
    "hashicorp",
    "icon",
    "svg",
    "flight"
  ],
  "repository": "https://github.com/hashicorp/flight/ember-flight-icons",
  "license": "MPL-2.0",
  "author": "HashiCorp Design Systems <design-systems@hashicorp.com>",
  "contributors": [
    {
      "name": "Melanie Sumner",
      "email": "melanie@hashicorp.com"
    },
    {
      "name": "Brian Runnells",
      "email": "brunnells@hashicorp.com"
    },
    {
      "name": "Hector Simpson",
      "email": "hector@hashicorp.com"
    },
    {
      "name": "Heather Larsen",
      "email": "hlarsen@hashicorp.com"
    },
    {
      "name": "Amy Lam",
      "email": "alam@hashicorp.com"
    },
    {
      "name": "Cristiano Rastelli",
      "email": "cristiano.rastelli@hashicorp.com"
    }
  ],
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel \"lint:!(fix)\"",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "npm-run-all lint test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each",
    "test:ember:percy": "percy exec ember test",
    "bump": "./node_modules/.bin/bump package.json",
    "release": "yarn publish --new-version $npm_package_version",
    "postrelease": "TAG_VERSION=\"@hashicorp/ember-flight-icons/${npm_package_version}\" && git tag $TAG_VERSION && git push origin $TAG_VERSION"
  },
  "dependencies": {
    "@hashicorp/flight-icons": "^1.1.0",
    "ember-cli-babel": "^7.26.6",
    "ember-cli-htmlbars": "^5.7.1",
    "ember-test-selectors": "^6.0.0"
  },
  "devDependencies": {
    "@ember/optional-features": "^2.0.0",
    "@ember/test-helpers": "^2.2.5",
    "@embroider/test-setup": "^0.41.0",
    "@glimmer/component": "^1.0.4",
    "@glimmer/tracking": "^1.0.4",
    "@percy/cli": "^1.0.0-beta.68",
    "@percy/ember": "^3.0.0",
    "@tailwindcss/forms": "^0.3.4",
    "@tailwindcss/typography": "^0.4.1",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "ember-auto-import": "^2.2.0",
    "ember-cli": "~3.27.0",
    "ember-cli-clipboard": "^0.15.0",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-postcss": "^7.0.2",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.2",
    "ember-composable-helpers": "^4.5.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-fetch": "^8.1.1",
    "ember-load-initializers": "^2.1.2",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-page-title": "^6.2.2",
    "ember-prism": "^0.11.0",
    "ember-qunit": "^5.1.4",
    "ember-resolver": "^8.0.2",
    "ember-source": "~3.26.1",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^3.5.1",
    "ember-template-lint-plugin-prettier": "^2.0.1",
    "ember-truth-helpers": "^3.0.0",
    "ember-try": "^1.4.0",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-ember": "^10.5.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-qunit": "^6.1.1",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "postcss-import": "^14.0.2",
    "prettier": "^2.4.1",
    "qunit": "^2.15.0",
    "qunit-dom": "^1.6.0",
    "tailwindcss": "^2.2.11",
    "version-bump-prompt": "^6.1.0",
    "webpack": "^5.52.1"
  },
  "engines": {
    "node": "12.* || >= 14"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  }
}

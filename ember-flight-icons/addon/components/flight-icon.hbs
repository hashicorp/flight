<svg 
  aria-hidden="true"
  class="flight-icon icon-{{@name}} {{if this.display "display-inline"}}"
  data-test-icon
  fill={{this.fillColor}} 
  height="{{@size}}"
  id={{this.iconId}}
  viewBox="0 0 {{@size}} {{@size}}"
  width="{{@size}}" 
  xmlns="http://www.w3.org/2000/svg"
>
  <use href='{{this.baseURL}}/ember-flight-icons/icons/{{@name}}-{{@size}}.svg'></use>
</svg>

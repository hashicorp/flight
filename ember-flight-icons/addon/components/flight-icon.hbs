<svg 
  aria-hidden="true"
  class="flight-icon icon-{{@name}} {{if this.display "display-inline"}}"
  data-test-icon
  fill={{this.fillColor}} 
  height="{{this.sizeNum}}"
  id={{this.iconId}}
  viewBox="0 0 {{this.sizeNum}} {{this.sizeNum}}"
  width="{{this.sizeNum}}" 
  xmlns="http://www.w3.org/2000/svg"
>
  {{component (concat 'flight-icon/' @name '-' this.size )}}
</svg>
